
<%= simple_form_for(@actual_expense) do |f| %>
  <%= f.error_notification %>
  <%= f.error_notification message: f.object.errors[:base].to_sentence if f.object.errors[:base].present? %>

  <div class="form-inputs">
    <%= f.input :type %>
    <%= f.input :total %>
    <%= f.association :request %>
    <%= f.input :status %>
    <%= f.input :dateincurred %>
    <%= #f.file_field :picture, id: :actual_expense_imageurl %>
    <%= #f.input :imageurl %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
